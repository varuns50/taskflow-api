{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node", "express"],
      "typeRoots": ["./src/types", "./node_modules/@types"] 
    },
    "include": ["src"]
  }
  